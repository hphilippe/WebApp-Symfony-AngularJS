<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>

        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

        <link href='http://fonts.googleapis.com/css?family=Raleway:200,400,800' rel='stylesheet' type='text/css'>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css">
        <link rel='stylesheet prefetch' href='http://www.marcoguglie.it/Codepen/AnimatedHeaderBg/demo-1/css/demo.css'>

        <!-- font -->
        <link href="https://fonts.googleapis.com/css?family=Orbitron" rel="stylesheet">

        {% block stylesheets %}
            {% stylesheets filter='cssrewrite, ?scssphp'
            'bundles/skearuser/css/style.css'
            'bundles/skearuser/css/surcharge_bootstrap.css' %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
            {% endstylesheets %}
        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>

        {% block body %}{% endblock %}
        {% block javascripts %}
            {% javascripts
            'bundles/skearuser/js/animatedLumiere.js' filter='?jsqueeze'
            'bundles/skearuser/js/moveImg.js' filter='?jsqueeze'%}
            <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}

    </body>
</html>
